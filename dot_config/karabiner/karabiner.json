{
    "global": {
        "check_for_updates_on_startup": true,
        "show_in_menu_bar": true,
        "show_profile_name_in_menu_bar": false,
        "ask_for_unknown_device_if_no_device_id_in_the_profile": false
    },
    "profiles": [
        {
            "complex_modifications": {
                "parameters": {
                    "basic.simultaneous_threshold_milliseconds": 50,
                    "basic.to_delayed_action_delay_milliseconds": 500,
                    "basic.to_if_alone_timeout_milliseconds": 1000,
                    "basic.to_if_held_down_threshold_milliseconds": 500,
                    "mouse_motion_to_scroll.speed": 100
                },
                "rules": [
                    {
                        "description": "Caps Lock as Super Key - Application Launcher",
                        "manipulators": [
                            {
                                "description": "Caps Lock + C → Calendar",
                                "from": {
                                    "key_code": "c",
                                    "modifiers": {
                                        "mandatory": ["caps_lock"]
                                    }
                                },
                                "to": [
                                    {
                                        "shell_command": "open -a 'Calendar'"
                                    }
                                ],
                                "type": "basic"
                            },
                            {
                                "description": "Caps Lock + F → Firefox",
                                "from": {
                                    "key_code": "f",
                                    "modifiers": {
                                        "mandatory": ["caps_lock"]
                                    }
                                },
                                "to": [
                                    {
                                        "shell_command": "open -a 'Firefox'"
                                    }
                                ],
                                "type": "basic"
                            },
                            {
                                "description": "Caps Lock + S → Slack",
                                "from": {
                                    "key_code": "s",
                                    "modifiers": {
                                        "mandatory": ["caps_lock"]
                                    }
                                },
                                "to": [
                                    {
                                        "shell_command": "open -a 'Slack'"
                                    }
                                ],
                                "type": "basic"
                            },
                            {
                                "description": "Caps Lock + V → Visual Studio Code",
                                "from": {
                                    "key_code": "v",
                                    "modifiers": {
                                        "mandatory": ["caps_lock"]
                                    }
                                },
                                "to": [
                                    {
                                        "shell_command": "open -a 'Visual Studio Code'"
                                    }
                                ],
                                "type": "basic"
                            },
                            {
                                "description": "Caps Lock + O → Obsidian",
                                "from": {
                                    "key_code": "o",
                                    "modifiers": {
                                        "mandatory": ["caps_lock"]
                                    }
                                },
                                "to": [
                                    {
                                        "shell_command": "open -a 'Obsidian'"
                                    }
                                ],
                                "type": "basic"
                            },
                            {
                                "description": "Caps Lock + Space → iTerm",
                                "from": {
                                    "key_code": "spacebar",
                                    "modifiers": {
                                        "mandatory": ["caps_lock"]
                                    }
                                },
                                "to": [
                                    {
                                        "shell_command": "open -a 'iTerm'"
                                    }
                                ],
                                "type": "basic"
                            }
                        ]
                    },
                    {
                        "description": "Firefox Tab Switching",
                        "manipulators": [
                            {
                                "conditions": [
                                    {
                                        "bundle_identifiers": [
                                            "org.mozilla.firefox"
                                        ],
                                        "type": "frontmost_application_if"
                                    }
                                ],
                                "description": "Caps Lock + M → Firefox Tab 1",
                                "from": {
                                    "key_code": "m",
                                    "modifiers": {
                                        "mandatory": ["caps_lock"]
                                    }
                                },
                                "to": [
                                    {
                                        "key_code": "1",
                                        "modifiers": ["command"]
                                    }
                                ],
                                "type": "basic"
                            },
                            {
                                "conditions": [
                                    {
                                        "bundle_identifiers": [
                                            "org.mozilla.firefox"
                                        ],
                                        "type": "frontmost_application_if"
                                    }
                                ],
                                "description": "Caps Lock + T → Firefox Tab 2",
                                "from": {
                                    "key_code": "t",
                                    "modifiers": {
                                        "mandatory": ["caps_lock"]
                                    }
                                },
                                "to": [
                                    {
                                        "key_code": "2",
                                        "modifiers": ["command"]
                                    }
                                ],
                                "type": "basic"
                            }
                        ]
                    }
